    <!-- commaseperated: form name,contact form,etc -->
    <setting key="IgnoreWorkFlowsOnEdit" value="True" />
    <!-- This can be 2 different things either True/False or a list of form names that will execute workflows async,  -->
    <!-- commaseperated: form name,contact form,etc -->
