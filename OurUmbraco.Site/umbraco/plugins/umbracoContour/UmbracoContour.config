    <setting key="DefaultManualApproval" value="False" />
    <setting key="DefaultDisableStylesheet" value="False" />
    <setting key="DefaultMarkFieldsIndicator" value="NoIndicator" />
    <setting key="DefaultIndicator" value="" />
